    "cleandev": "rm -rf node_modules pnpm-lock.yaml .nuxt&& pnpm i && pnpm dev",
    "predev": "node -v | grep -qE '^v20\\.' || (echo 'Error: Node.js 20 or higher is required.' && exit 1)",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare && pnpm build:internal",
    "preview": "nuxt preview",
    "tiny": "node ./internal/tinypng/index.js"
  },
